<?xml version="1.0" encoding="utf-8"?>
<ArrayOfUserCommand xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <UserCommand>
    <Title>サンプル: IEでコンテンツの画像を表示 (&amp;C)</Title>
    <FileName>C:\Program Files\Internet Explorer\iexplore.exe</FileName>
    <Arguments>{c:ImageLargeUri}</Arguments>
  </UserCommand>
  <UserCommand>
    <Title>サンプル: IEでパッケージの画像を表示 (&amp;P)</Title>
    <FileName>C:\Program Files\Internet Explorer\iexplore.exe</FileName>
    <Arguments>{p:ImageMiddleUri}</Arguments>
  </UserCommand>
  <UserCommand>
    <Title>サンプル: IEでジャンルのトップページを表示 (&amp;G)</Title>
    <FileName>C:\Program Files\Internet Explorer\iexplore.exe</FileName>
    <Arguments>{g:GenreTopPageUri}</Arguments>
  </UserCommand>
</ArrayOfUserCommand>